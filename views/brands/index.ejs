<% include ../partials/header %>

<div class="container">
	<div class="homeDiv" style="background-image: url('<%= images[0].imgData%>')" data-name="<%= images[0].instaName%>" data-complaint="<%= images[0].complaint%>"></div>
	<div class="homeDiv" style="background-image: url('<%= images[1].imgData%>')" data-name="<%= images[1].instaName%>" data-complaint="<%= images[1].complaint%>"></div>
